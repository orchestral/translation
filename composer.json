{
    "name": "orchestra/translation",
    "description": "Translation Component for Orchestra Platform",
    "homepage": "http://orchestraplatform.com/docs/latest/components/translation/",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "translation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Translation\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Translation\\TestCase\\" : "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "illuminate/translation": "~5.9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.9.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.9-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
